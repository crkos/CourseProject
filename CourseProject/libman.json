{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "Sortable@1.15.3",
      "destination": "wwwroot/lib/Sortable/"
    },
    {
      "provider": "jsdelivr",
      "library": "@bitheral/easymde@2.19.1",
      "destination": "wwwroot/lib/bitheral/easymde/"
    }
  ]
}